{
  "name": "lines-logger",
  "version": "1.0.7",
  "main": "lib/index.js",
  "description": "A simple browser logger that keeps origin source files location",
  "scripts": {
    "build": "",
    "test": "mocha",
    "report": "nyc mocha",
    "lcov": "nyc report --reporter=text-lcov > coverage.lcov",
    "codecov": "codecov",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "rm ./lib -rf && tsc -p src/tsconfig.json",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run check"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Deathangel908/lines-logger.git"
  },
  "keywords": [
    "logger"
  ],
  "files": [
    "lib"
  ],
  "typings": "./lib/index.d.ts",
  "author": "Andrew Koidan <deathangel908@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Deathangel908/lines-logger/issues"
  },
  "homepage": "https://github.com/Deathangel908/lines-logger#readme",
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.3",
    "@types/node": "^10.3.5",
    "chai": "^4.1.2",
    "nyc": "^13.3.0",
    "mocha": "^5.2.0",
    "ts-node": "^7.0.0",
    "typescript": "^2.9.2",
    "codecov": "^3.2.0",
    "istanbul": "^0.4.5",
    "gts": "^0.9.0"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  }
}
